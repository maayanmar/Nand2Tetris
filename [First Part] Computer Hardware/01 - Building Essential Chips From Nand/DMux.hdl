/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=in, b=sel, out=notb);
    Nand(a=in, b=notb, out=nota);
    Not(in=nota, out=a);
    Not(in=notb, out=b);
}
